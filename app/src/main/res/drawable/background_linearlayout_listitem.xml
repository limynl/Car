<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="#DFDFDF" android:endColor="#DFDFDF" android:centerColor="#DFDFDF" android:angle="90.0"/>
            <padding android:left="1.0dip" android:top="-0.5dip" android:right="1.0dip" android:bottom="-0.5dip"/>
        </shape>
    </item>

    <item android:state_selected="true">
        <shape>
            <gradient android:startColor="#DFDFDF" android:endColor="#DFDFDF" android:centerColor="#DFDFDF" android:angle="90.0"/>
            <padding android:left="1.0dip" android:top="-0.5dip" android:right="1.0dip" android:bottom="-0.5dip"/>
        </shape>
    </item>

    <item>
        <shape>
            <gradient android:startColor="#FFFFFF" android:endColor="#FFFFFF" android:centerColor="#FFFFFF" android:angle="90.0"/>
            <padding android:left="1.0dip" android:top="-0.5dip" android:right="1.0dip" android:bottom="-0.5dip"/>
        </shape>
    </item>
</selector>
